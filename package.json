{
  "name": "cli-multi-agent",
  "version": "0.1.0",
  "description": "CLI tool for multi-agent collaboration and debate",
  "type": "module",
  "bin": {
    "debate": "./bin/debate.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node ./dist/index.js",
    "debate": "node ./dist/index.js",
    "debug": "node --inspect ./dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:tmux": "npx tsx scripts/test-tmux.ts",
    "lint": "eslint src"
  },
  "keywords": [
    "cli",
    "agent",
    "ai",
    "debate",
    "collaboration",
    "tmux"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "ora": "^8.1.1",
    "winston": "^3.17.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
